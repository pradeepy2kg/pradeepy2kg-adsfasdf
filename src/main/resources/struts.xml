<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC  "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <include file="struts-default.xml"/>
    <constant name="struts.enable.DynamicMethodInvocation" value="false"/>
    <constant name="struts.custom.i18n.resources" value="textproperties"/>
    <constant name="struts.action.extension" value="do"/>
    <constant name="struts.objectFactory" value="spring"/>
    <constant name="struts.devMode" value="true"/>

    <package name="NG" namespace="/NG" extends="struts-default">
        <result-types>
            <result-type name="tiles" class="org.apache.struts2.views.tiles.TilesResult"/>
        </result-types>

        <interceptors>
            <interceptor name="exception" class="com.opensymphony.xwork2.interceptor.ExceptionMappingInterceptor"/>
            <interceptor name="security" class="lk.rgd.crs.web.action.SecurityInterceptor"/>
            <interceptor name="cacheControl" class="lk.rgd.crs.web.action.CacheControlInterceptor"/>
            <interceptor-stack name="default">
                <interceptor-ref name="exception"/>
                <interceptor-ref name="security"/>
                <interceptor-ref name="defaultStack"/>
                <interceptor-ref name="cacheControl"/>
            </interceptor-stack>
        </interceptors>

        <!--<default-interceptor-ref name="exception"/>-->
        <global-results>
            <result type="tiles" name="error">epr.error</result>
            <result type="tiles" name="RGDerror">epr.rgdError</result>
            <result type="tiles" name="none">epr.home</result>
            <result name="login">/WEB-INF/pages/login/login.jsp</result>
        </global-results>

        <global-exception-mappings>
            <exception-mapping exception="lk.rgd.common.RGDRuntimeException" result="RGDerror"></exception-mapping>
            <exception-mapping exception="java.lang.Exception" result="error"></exception-mapping>
        </global-exception-mappings>
    </package>

    <package name="epr" namespace="/" extends="NG">

        <action name="eprLogin" class="lk.rgd.crs.web.action.LoginAction" method="login">
            <interceptor-ref name="defaultStack"/>
            <interceptor-ref name="cacheControl"/>
            <result type="tiles" name="successADMIN">epr.home</result>
            <result type="tiles" name="successRG">epr.rghome</result>
            <result type="tiles" name="successARG">epr.home</result>
            <result type="tiles" name="successDR">epr.home</result>
            <result type="tiles" name="successADR">epr.adrhome</result>
            <result type="tiles" name="successDEO">epr.deohome</result>

            <result name="expired">/WEB-INF/pages/preferences/PasswordChange.jsp</result>
            <result name="error">/WEB-INF/pages/login/login.jsp</result>
        </action>

        <action name="eprLogout" class="lk.rgd.crs.web.action.LoginAction" method="logout">
            <interceptor-ref name="defaultStack"/>
            <interceptor-ref name="cacheControl"/>
            <result name="success">/WEB-INF/pages/login/login.jsp</result>
            <result type="tiles" name="error">epr.home</result>
        </action>

        <!--check here -->
        <action name="eprHome" class="lk.rgd.crs.web.action.births.BirthRegisterAction" method="welcome">
            <interceptor-ref name="default"/>
            <result type="tiles" name="success">epr.home</result>
        </action>

        <action name="ajaxSupport_*" class="lk.rgd.crs.web.action.AjaxAction" method="{1}">
            <result name="DSDivList">/WEB-INF/pages/template/DSDivBox.jsp</result>
            <result name="DSDivListOnly">/WEB-INF/pages/template/DSDivBoxOnly.jsp</result>
            <result name="BDDivList">/WEB-INF/pages/template/BDDivBox.jsp</result>
            <result name="FatherInfo">/WEB-INF/pages/template/FatherInfo.jsp</result>
            <result name="MotherInfo">/WEB-INF/pages/template/MotherInfo.jsp</result>
            <result name="DSDivListSearch">/WEB-INF/pages/template/DSDivBoxSearch.jsp</result>
            <result name="DSDivListBDFApproval">/WEB-INF/pages/template/DSDivBoxBDFApproval.jsp</result>
            <result name="DSDivListBDFConfirmation">/WEB-INF/pages/template/DSDivBoxBDFConfirmation.jsp</result>
            <result name="BirthCertificatePrint">/WEB-INF/pages/template/BCPrintDSDivBox.jsp</result>
            <result name="DSDivListBDFConfirmationPrint">/WEB-INF/pages/template/DSDivBoxBDFConfirmationPrint.jsp
            </result>
            <result name="DSDivListUserPreference">/WEB-INF/pages/template/DSDivBoxUserPreference.jsp</result>
            <result name="MotherDSDivList">/WEB-INF/pages/template/DSDivBoxMother.jsp</result>
        </action>
    </package>

    <package name="prs" namespace="/prs" extends="NG">
        <default-interceptor-ref name="default"/>

        <action name="eprPRSAdvancedSearch" class="lk.rgd.crs.web.action.births.SearchAction"
                method="advancedSearch">
            <result type="tiles" name="success">epr.PRSAdvancedSearch</result>
            <result type="tiles" name="error">epr.error</result>
        </action>
